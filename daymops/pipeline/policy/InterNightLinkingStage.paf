#<?cfg paf policy ?>
# Default TelescopeSimulatorStage policy file
# All angles in degrees. All times are in days.
databasePolicy: @policy/DataBase.paf
locationPolicy: @policy/Location.paf

minNights: 3                # minimum number of nights to attempt OD
timeSpan: 30                # max number of days worth of data to consider.

numRangingOrbits: 5000      # number of ranging orbits to use.
elementType: 'keplerian'    # type of orbital elements to use.
stdDev: 8.3333333e-05       # observational error in degrees.

slowMinV: 0.0               # minv for "slow movers"
slowMaxV: 0.5               # maxv for "slow movers"
slowVtreeThresh: 0.0003
slowPredThresh: 0.0005

fastMinV: 0.4               # minv for "fast movers"
fastMaxV: 7.0               # maxv for "fast movers"
fastVtreeThresh: 0.0025
fastPredThresh: 0.0025

plateWidth: .0001
#
